{
  "bail": false,
  "rootDir": "../../",
  "testRegex": "(\\.(test))\\.js$",
  "moduleFileExtensions": [
    "js",
    "json"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/"
  ],
  "setupFiles": [
    "dotenv/config"
  ],
  "setupFilesAfterEnv": ["./test/framework.js"],
  "transform": {
    ".*\\.js$": "babel-jest"
  }
}
